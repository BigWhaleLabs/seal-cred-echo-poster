    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "ethers": "^5.6.9",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^11.0.1",
    "mongoose": "^6.4.5",
